{
    "services":{
        "auth":{
            "basePath": "/auth",
            "routes": [
                {
                    "path":"/login",
                    "method": "POST",
                    "handler": "login"
                }
            ]
        },
        "dataTable":{
            "basePath": "/data-table",
            "routes": [
                {
                    "path":"",
                    "method": "POST",
                    "handler": "getTableData"
                }
            ]
        },
        "dashboard":{
            "basePath": "/dashboard",
            "routes": [
                {
                    "path":"/data",
                    "method": "GET",
                    "handler": "getDashboardData"
                }
            ]
        }
    }
}